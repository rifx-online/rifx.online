{
  "site": {
    "title": "Rifx.Online",
    "base_url": "https://rifx.online",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/favicon.svg",
    "logo": "/images/logo.svg",
    "logo_darkmode": "/images/logo-darkmode.png",
    "logo_width": "150",
    "logo_height": "30",
    "logo_text": "Rifx.Online"
  },

  "settings": {
    "search": true,
    "sticky_header": true,
    "theme_switcher": true,
    "default_theme": "system",
    "pagination": 15,
    "summary_length": 200,
    "blog_folder": "blog",
    "model_folder": "models",
    "default_language": "en",
    "disable_languages": ["fr", "zh"],
    "default_language_in_subdir": false
  },

  "categories": [
    "blog",
    "models"
  ],

  "params": {
    "contact_form_action": "#",
    "copyright": "Designed And Developed by [Zeon Studio](https://zeon.studio)"
  },

  "disqus": {
    "enable": true,
    "shortname": "themefisher-template",
    "settings": {}
  },

  "metadata": {
    "meta_author": "zeon.studio",
    "meta_image": "/images/og-image.png",
    "meta_description": "astro and tailwind boilerplate"
  }
}
